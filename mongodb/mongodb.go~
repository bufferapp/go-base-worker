package mongodb

import (
	"gopkg.in/mgo.v2"
)

// Initialize mongo connection
func Initialize(connectionString string, collection string) (*mgo.Session, error) {
	session, err := mgo.Dial("server1.example.com,server2.example.com")
	if err != nil {
			panic(err)
	}
	defer session.Close()
	return session
}
